{
    "documentId": "44eccb8c34362883e6cd3fd1",
    "assemblyName": "URDF_Assembly_LD_Robot",
    "outputFormat": "urdf",
    "output_filename": "robot_harambe",
    // Output filename (default: "robot")
    // Extension (.urdf, .xml) will be added automatically
    // Assets directory (default: "assets")
    "assets_directory": "assets",
    // Assembly name to use in the document (optional)
    // Robot name (default: "onshape")
    "robot_name": "robot_harambe",
    // // Parts to ignore (default: {})
    //     "ignore": {
    //         // Ignore visual for visual
    //         "part1": "visual",
    //         "screw*": "visual",
    //         // Ignore everything expect "leg" for collision
    //         "*" : "collision"
    //         "!leg": "collision"
    //     },
    "ignore": {
        "stator_*": "collision",
        "rotor_*": "collision",
        "m6_threaded*": "collision",
        "hex_nut*": "collision",
        "12_7mm_ball*": "collision",
        "rulment*": "collision",
        "ankle_bearing_house*": "collision",
        "spacer*": "collision",
        "lever_ankle*": "collision",
        "lower_fixation*": "collision"
    },
    // Collision meshes directory (used by simplify_collisions.py)
    "collision_assets_directory": "collision",
    "simplify_stls": true,
    // Maximum size of the STL files in MB (default: 3)
    "max_stl_size": 0.4
}
